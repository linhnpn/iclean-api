<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.iclean.icleanapi.dao.ProductMapper">

    <select id="getProductByID" parameterType="com.iclean.icleanapi.domain.Product" resultType="com.iclean.icleanapi.domain.Product">
        SELECT id,
        name
        FROM products WHERE id = #{id}
    </select>
</mapper>